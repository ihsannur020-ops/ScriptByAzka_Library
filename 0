local gameScripts = {
    [119987266683883] = "https://raw.githubusercontent.com/ihsannur020-ops/ScriptByAzka_Library/main/SurviveLavaForBrainrot", --Survival Lava For Brainrot
    [137723497878181] = "https://raw.githubusercontent.com/ihsannur020-ops/ScriptByAzka_Library/refs/heads/main/EscapeRissingLavaForBrainrot", --Escape Rissing Lava For Brainrot
    [94702395375549] = "https://raw.githubusercontent.com/ihsannur020-ops/ScriptByAzka_Library/refs/heads/main/RunForBrainrots"
    
}

local currentGameId = game.PlaceId
local scriptUrl = gameScripts[currentGameId]

if scriptUrl then
    local success, content = pcall(game.HttpGet, game, scriptUrl)
    if success and content then
        local loadSuccess, func = pcall(loadstring, content)
        if loadSuccess and func then
            pcall(func)
        end
    end
end
